<Page x:Class="Task2.Pages.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Task2.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="StartPage">

    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Button x:Name="registrationButton" Content="Зарегистрироваться" Margin="5 5 5 5" Click="RegistrationButton_Click"/>
        <Button x:Name="authorizationButton" Content="Войти" Margin="5 5 5 5" Click="AuthorizationButton_Click"/>
    </StackPanel>
</Page>
