<Window x:Class="Task5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task5"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Button x:Name="PrintButton" Content="Печать" Click="PrintButton_Click"/>
        <StackPanel>
            <Canvas x:Name="canvas">
                <Label Content="Поле ввода"/>
                <TextBox AcceptsReturn="True" Width="600" Height="300" Canvas.Top="25"/>
                <Image Source="/background.jpg" Height="100" Canvas.Right="50"/>
            </Canvas>
        </StackPanel>
    </StackPanel>
</Window>
